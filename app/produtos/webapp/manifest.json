{
  "_version": "1.76.0",
  "sap.app": {
    "id": "comp.container.com.produtos",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.18.6",
      "toolsId": "ff3ff9c3-c022-4dd8-87e5-c19063b8cd6a"
    },
    "dataSources": {
      "mainService": {
        "uri": "/Produtos/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ZPRODUTOS-manage": {
          "semanticObject": "ZPRODUTOS",
          "action": "manage",
          "title": "{{ZPRODUTOS-manage.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "comp.container.com.produtos.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": "Produtos",
          "name": "ProdutosList",
          "target": "ProdutosList"
        },
        {
          "pattern": "Produtos({key}):?query:",
          "name": "ProdutosObjectPage",
          "target": "ProdutosObjectPage"
        }
      ],
      "targets": {
        "ProdutosList": {
          "type": "Component",
          "id": "ProdutosList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Produtos",
              "variantManagement": "Page",
              "navigation": {
                "Produtos": {
                  "detail": {
                    "route": "ProdutosObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  },
                  "columns": {
                    "DataField::ID": {
                      "width": "20%"
                    },
                    "DataField::nome": {
                      "width": "20%"
                    },
                    "DataField::peso": {
                      "width": "20%"
                    },
                    "DataField::preco": {
                      "width": "20%"
                    },
                    "DataField::um": {
                      "width": "20%"
                    }
                  }
                }
              },
              "initialLoad": "Enabled"
            }
          }
        },
        "ProdutosObjectPage": {
          "type": "Component",
          "id": "ProdutosObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Produtos"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "comp.container.router"
  }
}
