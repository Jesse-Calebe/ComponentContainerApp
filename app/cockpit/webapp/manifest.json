{
  "_version": "1.76.0",
  "sap.app": {
    "id": "comp.container.com.cockpit",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.6",
      "toolsId": "2b51d838-dd7c-4d6d-a0ad-65451f3776f9"
    },
    "dataSources": {
      "mainService": {
        "uri": "/Home/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ZCOCKPIT-manage": {
          "semanticObject": "ZCOCKPIT",
          "action": "manage",
          "title": "{{ZCOCKPIT-manage.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "comp.container.com.cockpit.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "comp.container.com.cockpit.view",
        "async": true,
        "viewPath": "comp.container.com.cockpit.view"
      },
      "routes": [
        {
          "name": "RouteCockpit",
          "pattern": ":?query:",
          "target": [
            "TargetCockpit"
          ]
        },
        {
          "name": "RouteCockpitProdutos",
          "pattern": "Produtos",
          "target": [
            "TargetCockpit"
          ]
        },
        {
          "name": "RouteCockpitClientes",
          "pattern": "Clientes",
          "target": [
            "TargetCockpit"
          ]
        }
      ],
      "targets": {
        "TargetCockpit": {
          "id": "Cockpit",
          "name": "Cockpit"
        }
      }
    },
    "rootView": {
      "viewName": "comp.container.com.cockpit.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "comp.container.router"
  }
}
